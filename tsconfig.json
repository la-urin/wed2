{
    "compilerOptions": {
        "outDir": "./built",
        "strict": true,
        "target": "ES6",
        "module": "ES6",
        "allowJs": true,
        "checkJs": true,
        "moduleResolution": "node",
        "noImplicitAny": false,
        "allowSyntheticDefaultImports": true
    },
    "exclude": [
        "node_modules"
    ]
}
